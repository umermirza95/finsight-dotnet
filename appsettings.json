{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host={IP};Database={DB};Username={USER};Password=PASSWORD"
  },
  "Jwt": {
    "Key": "{JWT_KEY}",
    "Issuer": "{JWT_ISSUER}",
    "Audience": "{JWT_AUDIENCE}"
  },
  "ExchangeRateApi": {
    "BaseUrl": "{API_URL}",
    "ApiKey": "{YOUR_SECRET_TOKEN_HERE}"
  }
}
